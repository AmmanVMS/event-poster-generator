{
  "name": "event-poster-generator",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "docs:clean": "rm -rf docs/",
    "docs:restore": "npm run docs:clean && git restore docs/",
    "generate": "ts-node ./scripts/version.ts && npm run docs:clean && nuxt generate && git add ./docs/ ./version.json && git commit -m '[generate]docs'",
    "generate2": "ts-node ./scripts/version.ts && npm run docs:clean && nuxt generate "
  },
  "dependencies": {
    "core-js": "^3.26.0",
    "element-ui": "^2.15.10",
    "markdown-it": "^13.0.1",
    "nuxt": "^2.13.3",
    "qrcode": "^1.5.1",
    "retina-dom-to-image": "^2.5.6",
    "vue-async-computed": "^3.9.0"
  },
  "devDependencies": {
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^11.0.0",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.11.9",
    "@types/qrcode": "^1.5.0",
    "@types/semver": "^7.3.13",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "prettier-eslint": "^15.0.1",
    "rimraf": "^3.0.2",
    "sass": "^1.56.1",
    "semver": "^7.3.8",
    "ts-node": "^10.9.1"
  }
}
